# API Configuration
API_BASE_URL=http://localhost:3000
API_TIMEOUT=30000

# Ola Maps Configuration
OLA_MAPS_API_KEY=your_ola_maps_api_key
OLA_MAPS_API_SECRET=your_ola_maps_api_secret

# App Configuration
APP_NAME=NATPAC Travel
APP_VERSION=1.0.0
APP_BUILD=1

# Feature Flags
ENABLE_KERALA_FEATURES=true
ENABLE_OFFLINE_MODE=true
ENABLE_ANALYTICS=true
ENABLE_CRASH_REPORTING=true

# Google Services (for Android)
GOOGLE_SERVICES_API_KEY=your_google_services_key

# Sentry Configuration
SENTRY_DSN=your_sentry_dsn_for_mobile

# Deep Linking
APP_SCHEME=natpactravel
APP_DOMAIN=natpactravel.app

# Push Notifications
FCM_SERVER_KEY=your_fcm_server_key
APNS_KEY_ID=your_apns_key_id

# Analytics
ANALYTICS_TRACKING_ID=your_analytics_id

# Debug Settings
ENABLE_NETWORK_LOGS=false
ENABLE_REDUX_DEVTOOLS=false